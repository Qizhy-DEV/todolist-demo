    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.3.1",
        "husky": "^9.1.7",
        "prettier": "^3.5.3",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
